

_papis (){
local cur
# Pointer to current completion word.
# By convention, it's named "cur" but this isn't strictly necessary.

COMPREPLY=()   # Array variable storing the possible completions.
cur=${COMP_WORDS[COMP_CWORD]}
prev=${COMP_WORDS[COMP_CWORD-1]}

case "$cur" in
  -*)
  COMPREPLY=( $( compgen -W " -c	 --cc	 --clear-cache	 --config	 --cores	 -h	 --help	 -j	 -l	 --lib	 --log	 --pick-lib	 --picktool	 --set	 -v	 -V	 --verbose	 --version" -- $cur ) );;
  * )
  COMPREPLY=( $( compgen -W "add	check	config	edit	export	explore	list	rm	mv	rename	open	browse	update	run	git	gui	sync	zotero" -- $cur ) );;
esac

case "$prev" in

  config)
    COMPREPLY=( $( compgen -W " -h	 --help file-name	browse-query-format	notes-name	mark-header-format	default-library	export-text-format	cache-dir	add-open	match-format	check-keys	file-browser	mark-key-name	info-name	search-engine	scripts-short-help-regex	xeditor	opentool	extra-bibtex-types	sync-command	doc-url-key-name	use-cache	mark-format-name	mark-match-format	browser	user-agent	info-allow-unicode	picktool	default-gui	add-edit	header-format	add-confirm	use-git	local-config-file	editor	open-mark	ref-format	add-name	mode	database-backend	add-interactive	mvtool	extra-bibtex-keys	mark-opener-format	dir-umask	format-doc-name	urwid-gui-prompt-key	urwid-gui-quit-key	urwid-gui-kill-buffer-key	urwid-gui-help-key	urwid-gui-show-fields	urwid-gui-redraw-key	urwid-gui-search-key	rofi-gui-sep	rofi-gui-key-open-stay	rofi-gui-eh	rofi-gui-fullscreen	rofi-gui-normal_window	rofi-gui-key-quit	rofi-gui-multi_select	rofi-gui-key-open	rofi-gui-key-normal-window	rofi-gui-markup_rows	rofi-gui-key-edit	rofi-gui-fixed_lines	rofi-gui-header-format	rofi-gui-key-browse	rofi-gui-case_sensitive	rofi-gui-lines	rofi-gui-key-delete	rofi-gui-width	rofi-gui-key-help	tk-gui-print_info	tk-gui-cursor	tk-gui-activeforeground	tk-gui-open	tk-gui-borderwidth	tk-gui-match-format	tk-gui-move_bottom	tk-gui-clear	tk-gui-entry-lines	tk-gui-entry-bg-odd	tk-gui-entry-bg-size	tk-gui-entry-fg	tk-gui-help	tk-gui-prompt-fg	tk-gui-header-format	tk-gui-edit	tk-gui-half_up	tk-gui-window-bg	tk-gui-entry-bg-pair	tk-gui-entry-font-style	tk-gui-scroll_up	tk-gui-height	tk-gui-window-height	tk-gui-exit	tk-gui-prompt-bg	tk-gui-focus_prompt	tk-gui-activebackground	tk-gui-move_top	tk-gui-prompt-font-size	tk-gui-move_down	tk-gui-move_up	tk-gui-labels-per-page	tk-gui-entry-font-size	tk-gui-half_down	tk-gui-entry-font-name	tk-gui-window-width	tk-gui-scroll_down	tk-gui-insertbackground	vim-gui-header-format	vim-gui-open-dir-key	vim-gui-search-key	vim-gui-edit-key	vim-gui-help-key	vim-gui-next-search-key	vim-gui-delete-key	vim-gui-prev-search-key	vim-gui-open-key" -- $cur ) )
    ;;

  add)
    COMPREPLY=( $( compgen -W " --author	 --commit	 --confirm	 -d	 --dir	 --edit	 --file-name	 --from-bibtex	 --from-doi	 --from-folder	 --from-lib	 --from-pmid	 --from-url	 --from-vcf	 --from-yaml	 -h	 --help	 -i	 --interactive	 --name	 --no-document	 --open	 --title	 --to" -- $cur ) )
    ;;

  check)
    COMPREPLY=( $( compgen -W " -h	 --help	 -k	 --keys" -- $cur ) )
    ;;

  config)
    COMPREPLY=( $( compgen -W " -h	 --help" -- $cur ) )
    ;;

  edit)
    COMPREPLY=( $( compgen -W " -h	 --help	 -n	 --notes" -- $cur ) )
    ;;

  export)
    COMPREPLY=( $( compgen -W " -a	 --all	 --bibtex	 --file	 --folder	 -h	 --help	 --json	 --no-bibtex	 -o	 --out	 -t	 --text	 --vcf	 --yaml" -- $cur ) )
    ;;

  explore)
    COMPREPLY=( $( compgen -W " --add	 --arxiv	 -h	 --help	 --isbnplus	 --libgen	 --max" -- $cur ) )
    ;;

  list)
    COMPREPLY=( $( compgen -W " -d	 --dir	 --downloaders	 -f	 --file	 --format	 -h	 --help	 -i	 --info	 --libraries	 -p	 --pick	 --template" -- $cur ) )
    ;;

  rm)
    COMPREPLY=( $( compgen -W " -f	 --file	 --force	 -h	 --help" -- $cur ) )
    ;;

  mv)
    COMPREPLY=( $( compgen -W " --git	 -h	 --help" -- $cur ) )
    ;;

  rename)
    COMPREPLY=( $( compgen -W " --git	 -h	 --help" -- $cur ) )
    ;;

  open)
    COMPREPLY=( $( compgen -W " --all	 -d	 --dir	 -h	 --help	 -m	 --mark	 --tool" -- $cur ) )
    ;;

  browse)
    COMPREPLY=( $( compgen -W " -h	 --help" -- $cur ) )
    ;;

  update)
    COMPREPLY=( $( compgen -W " --auto	 -d	 --document	 -f	 --force	 --from-bibtex	 --from-doi	 --from-isbnplus	 --from-url	 --from-yaml	 -h	 --help	 -i	 --interactive" -- $cur ) )
    ;;

  run)
    COMPREPLY=( $( compgen -W " -h	 --help" -- $cur ) )
    ;;

  git)
    COMPREPLY=( $( compgen -W " -h	 --help" -- $cur ) )
    ;;

  gui)
    COMPREPLY=( $( compgen -W " -h	 --help	 --rofi	 --tk	 --urwid	 --vim" -- $cur ) )
    ;;

  sync)
    COMPREPLY=( $( compgen -W " -h	 --help" -- $cur ) )
    ;;

  zotero)
    COMPREPLY=( $( compgen -W "	--absolute		--relative" -- $cur ) )
    ;;


  --picktool)
    COMPREPLY=( $( compgen -W "papis.pick vim rofi" -- $cur ) )
    ;;

  --log)
    COMPREPLY=( $( compgen -W "INFO DEBUG WARNING ERROR CRITICAL" -- $cur ) )
    ;;

  --out|-o)
    COMPREPLY=( $( compgen -f -- $cur ) )
    ;;

  --from-*)
    COMPREPLY=( $( compgen -f -- $cur ) )
    ;;

  --lib|-l|--from-lib)
    COMPREPLY=( $( compgen -f -W "$(papis list --libraries)" -- $cur ) )
    ;;

  --set)
    COMPREPLY=( $( compgen -W "default-library=	file-name=	mark-format-name=	scripts-short-help-regex=	info-allow-unicode=	database-backend=	local-config-file=	cache-dir=	match-format=	search-engine=	mark-key-name=	extra-bibtex-types=	open-mark=	add-confirm=	add-interactive=	header-format=	ref-format=	info-name=	doc-url-key-name=	file-browser=	mode=	check-keys=	mark-match-format=	notes-name=	use-git=	extra-bibtex-keys=	dir-umask=	add-open=	export-text-format=	mvtool=	editor=	browse-query-format=	sync-command=	opentool=	add-name=	add-edit=	mark-opener-format=	user-agent=	xeditor=	default-gui=	picktool=	format-doc-name=	use-cache=	browser=	mark-header-format=	vim-gui-prev-search-key=	vim-gui-open-key=	vim-gui-next-search-key=	vim-gui-header-format=	vim-gui-delete-key=	vim-gui-search-key=	vim-gui-open-dir-key=	vim-gui-help-key=	vim-gui-edit-key=	tk-gui-clear=	tk-gui-half_down=	tk-gui-move_top=	tk-gui-scroll_down=	tk-gui-entry-fg=	tk-gui-move_up=	tk-gui-cursor=	tk-gui-half_up=	tk-gui-entry-bg-pair=	tk-gui-borderwidth=	tk-gui-match-format=	tk-gui-height=	tk-gui-window-height=	tk-gui-open=	tk-gui-move_down=	tk-gui-help=	tk-gui-entry-font-size=	tk-gui-entry-bg-odd=	tk-gui-edit=	tk-gui-window-bg=	tk-gui-activeforeground=	tk-gui-prompt-fg=	tk-gui-move_bottom=	tk-gui-prompt-bg=	tk-gui-entry-bg-size=	tk-gui-labels-per-page=	tk-gui-header-format=	tk-gui-scroll_up=	tk-gui-insertbackground=	tk-gui-focus_prompt=	tk-gui-entry-lines=	tk-gui-print_info=	tk-gui-entry-font-style=	tk-gui-prompt-font-size=	tk-gui-activebackground=	tk-gui-entry-font-name=	tk-gui-exit=	tk-gui-window-width=	rofi-gui-key-open=	rofi-gui-normal_window=	rofi-gui-key-quit=	rofi-gui-key-delete=	rofi-gui-lines=	rofi-gui-case_sensitive=	rofi-gui-markup_rows=	rofi-gui-sep=	rofi-gui-header-format=	rofi-gui-key-open-stay=	rofi-gui-multi_select=	rofi-gui-width=	rofi-gui-key-normal-window=	rofi-gui-key-browse=	rofi-gui-eh=	rofi-gui-key-help=	rofi-gui-key-edit=	rofi-gui-fixed_lines=	rofi-gui-fullscreen=	urwid-gui-prompt-key=	urwid-gui-quit-key=	urwid-gui-redraw-key=	urwid-gui-search-key=	urwid-gui-show-fields=	urwid-gui-help-key=	urwid-gui-kill-buffer-key=" -- $cur ) )
    ;;

esac

return 0
}


complete -F _papis papis
